function question() {
    $( "span,div" ).show( "slow" );
  }
  // Can pass in function name
  $( "button" ).click( question );
   
  $( "form" ).submit(function( event ) {
    if ( $( "input" ).val() === "yes" ) {
      $( "p" ).show( 4000, function() {
        $( this ).text( "Correct or Wrong" );
      });
    }
    $( "span,div" ).hide( "fast" );



